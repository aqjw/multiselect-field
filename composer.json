{
    "name": "aqjw/multiselect-field",
    "description": "A multiple select field for Laravel Nova.",
    "keywords": [
        "laravel",
        "nova",
        "multiselect",
        "select"
    ],
    "authors": [
        {
            "name": "Anton Shever",
            "email": "anton.shever96@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "laravel/nova": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "Aqjw\\MultiselectField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aqjw\\MultiselectField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ]
}
